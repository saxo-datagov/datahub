namespace com.linkedin.dataset

import com.linkedin.common.DatasetFieldUrn
import com.linkedin.business.BusinessTermLink

/**
 * Representation of mapping between fields in source dataset to the business term
 */
record DatasetFieldBusinessTerm {
  /**
   * Source fields from which the fine grained lineage is derived
   */
  sourceField: DatasetFieldUrn

  /**
   * Destination field which is derived from source fields
   */
  businessTerm: BusinessTermLink
}
